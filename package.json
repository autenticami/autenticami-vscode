{
  "name": "autenticami",
  "displayName": "Autenticami",
  "description": "Autenticami a Multi-Account and Multi-Tenant Authorization Provider (OpenSource + SaaS and SelfHosted).",
  "publisher": "autenticami",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com:autenticami/autenticami-vscode"
  },
  "icon": "assets/autenticami-icon.png",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.86.0"
  },
  "scripts": {
    "deploy": "vsce publish"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "autenticami",
      "aliases": ["autenticami", "autenticami"],
      "extensions": ["aut", "authz"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./assets/autenticami-ext.png",
        "dark": "./assets/autenticami-ext.png"
      }
    }],
    "grammars": [{
      "language": "autenticami",
      "scopeName": "source.aut",
      "path": "./syntaxes/autenticami.tmLanguage.json"
    }]
  }
}
